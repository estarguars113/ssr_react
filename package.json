    "axios": "0.18.0",
    "compression": "1.7.3",
    "concurrently": "4.0.1",
    "express": "4.16.3",
    "express-http-proxy": "1.4.0",
    "lodash": "4.17.11",
    "nodemon": "1.18.4",
    "npm-run-all": "4.1.3",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.7",
    "react-router-config": "1.0.0-beta.4",
